<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ListaDeComprasApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ListaDeComprasApp"
    Title="ListaDeComprasApp">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Produtos"
        ContentTemplate="{DataTemplate local:ProdutoPage}"
        Route="ProdutoPage" />

</Shell>
